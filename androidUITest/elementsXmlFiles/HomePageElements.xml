	<elements name="tabMenuTitle" id="com.arisan1000.android:id/tab_view_item_title">
		<element name="indexTitle" index="0">
			<description>index tab标签文案</description>
			<text>
				<language name="english">Index</language>
				<language name="indonesal">Utama</language>
			</text>
		</element>
		
		<element name="eventTitle" index="1">
			<description>event tab标签文案</description>
			<text>
				<language name="english">Event</language>
				<language name="indonesal">Event</language>
			</text>
		</element>
		
		<element name="offerTitle" index="2">
			<description>Offer tab标签文案</description>
			<text>
				<language name="english">Offer</language>
				<language name="indonesal">Offer</language>
			</text>
		</element>
		
		<element name="meTitle" index="3">
			<description>me tab标签文案</description>
			<text>
				<language name="english">Me</language>
				<language name="indonesal">Akun</language>
			</text>
		</element>
		
	</elements>
	
	<element name="settingIcon">
	 	<description>设置按钮icon</description>
		<resource-id>com.arisan1000.android:id/app_bar_navigation</resource-id>
	</element>
	
	<element name="rechargeIcon">
	 	<description>充值按钮icon</description>
		<resource-id>com.arisan1000.android:id/menu_home_top_up</resource-id>
	</element>
	
	<elements name="buyBtn" id="com.arisan1000.android:id/btn">
		<resource-id>商品列表每个商品图的购买按钮</resource-id>
	</elements>
	
	<elements name="productIcon" id="com.arisan1000.android:id/icon">
		<resource-id>商品列表每个商品图</resource-id>
	</elements>
	
	<element name="checkInCloseBtn">
	 	<description>签到弹框的关闭按钮</description>
		<resource-id>com.arisan1000.android:id/check_in_close_iv</resource-id>
	</element>
