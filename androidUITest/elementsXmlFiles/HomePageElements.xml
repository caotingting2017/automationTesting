<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE class [
<!ATTLIST element name ID #REQUIRED>
<!ATTLIST element index CDATA #IMPLIED>
]>

<class name="GuidePageElements">
	<!-- element 标签中，属性name的值唯一 -->

	<!-- 第一次启动时，新手大礼包弹框 -->
	<element name="noticeStr">
		<description>弹框说明文案</description>
		<resource-id>com.arisan1000.android:id/tv_novice_gift_package</resource-id>
		<text>
			<language name="english">Sign in to get free vouchers valued up to
				$50, and have a chance to get the amazing products.</language>
			<language name="indonesal">Daftar- Masuk dan dapatkan Voucher Cashback
				senilai Rp 50.000
				Langsung gunakan untuk memperoleh Produk Favoritmu</language>
		</text>
	</element>

	<element name="receiveAwardBtn">
		<description>弹框的确定按钮</description>
		<resource-id>com.arisan1000.android:id/btn_receive_award</resource-id>
		<text>
			<language name="english">Get it Now</language>
			<language name="indonesal">Daftar Akun</language>
		</text>
	</element>

	<element name="closeBtn">
		<description>弹框的关闭按钮</description>
		<resource-id>com.arisan1000.android:id/close</resource-id>

	</element>

	<!-- Home页面 -->
	<element name="homePage">
		<description>Home页面</description>
		<resource-id>com.arisan1000.android:id/main</resource-id>

	</element>

	<element name="AppTitle">
		<description>App首页标题</description>
		<resource-id>com.arisan1000.android:id/title_bar_title</resource-id>
		<text>
			<language name="english">One Dollar Get</language>
			<language name="indonesal">Arisan 1000</language>
		</text>
	</element>
	<element name="end">
		<description>开始菜单</description>
		<name>start</name>
		<resource-id>start</resource-id>
		<content-desc>start</content-desc>
		<text>
			<language name="english">start</language>
			<language name="indonesal">Mulai</language>
		</text>
	</element>

	<!-- 定位标识相同的一批元素，id为元素的定位标识 -->
	<!-- 首页的导航菜单 -->
	<elements name="navigationIcon" id="com.arisan1000.android:id/entry_iv" />

	<!-- 菜单上的标题 -->
	<elements name="navigationTitle" id="com.arisan1000.android:id/entry_title_tv">
		<element name="share" index="0">
			<description>分享</description>
			<text>
				<language name="english">Share</language>
				<language name="indonesal">Share</language>
			</text>
		</element>

		<element name="luckyWheel" index="1">
			<description>大转盘</description>
			<text>
				<language name="english">luckwheel</language>
				<language name="indonesal">Roda Hoki</language>
			</text>
		</element>

		<element name="recharge" index="2">
			<description>充值</description>
			<text>
				<language name="english">Recharge</language>
				<language name="indonesal">Top-Up</language>
			</text>
		</element>

		<element name="help" index="3">
			<description>帮助页面</description>
			<text>
				<language name="english">Recharge</language>
				<language name="indonesal">Bantuan</language>
			</text>
		</element>
	</elements>
	
	<!-- Tab功能菜单 -->
